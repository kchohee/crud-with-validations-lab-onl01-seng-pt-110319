 <h1> Song Index</h1> 
<tbody>
    <% @songs.each do |song| %>
        <tr>
            <td><%= song.title %></td>
            <td><%= song.released %></td>
            <td><%= song.release_year %></td>
            <td><%= song.artist_name %></td>
            <td><%= song.genre %></td>
            <td><%= link_to 'Delete', song, method: :delete, data: {confirm: 'You sure?'} %></td>
            <br>
        </tr>
    <% end %>
</tbody>